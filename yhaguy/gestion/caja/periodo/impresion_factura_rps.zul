		</div>
		<div width="94px">
			<label value="${arg.Vendedor.toString().toUpperCase()}" maxlength="30" sclass="det"></label>
		</div>	
		<div width="94px"></div>
		<div>
			<label value="${arg.FechaEmision}" sclass="det"></label>
		</div>
		<div width="75px"></div>
	</hlayout>
	
	<hlayout>
		<div width="102px"></div>
		<div width="320px">
			<label value="${arg.Telefono.toString().toUpperCase()}" maxlength="38" sclass="det"></label>
		</div>	
		<div width="94px">
		</div>	
		<div width="94px"></div>
		<div>
			<label value="${arg.Vencimiento}" sclass="det" visible="@load(vm.selectedVenta.condicionContado? 'false':'true')"></label>
		</div>
		<div width="75px"></div>
	</hlayout>
	
	<div height="25px"></div>
	
	<vbox children="@load(vm.selectedVenta.detallesConDescuento)" spacing="0px" height="100px">
		<template name="children">
			<hbox spacing="0px">
				<div width="35px"></div>
				<div width="50px" style="text-align:center">
					<label value="@load(each.cantidad)" sclass="det"></label>
				</div>	
				<div width="18px"></div>
				<div width="100px">
					<label value="@load(each.articulo.pos1.toString().toUpperCase())" sclass="det"></label>
				</div>	
				<div width="250px">
					<label value="@load(each.articulo.pos4.toString().toUpperCase())" sclass="det" maxlength="60"></label>
				</div>	
				<div width="65px" style="text-align:right">
					<label value="@load(each.precioGs) @converter(vm.cnv.monedaLocal)" sclass="det"></label>
				</div>	
				<div width="70px" style="text-align:right">
					<label value="@load(each.exenta ? each.importeGsSinDescuento : 0) @converter(vm.cnv.monedaLocal)" sclass="det"></label>
				</div>
				<div width="80px" style="text-align:right">
					<label value="0" sclass="det"></label>
				</div>
				<div width="105px" style="text-align:right">
					<label value="@load(each.exenta ? 0 : each.importeGsSinDescuento) @converter(vm.cnv.monedaLocal)" sclass="det"></label>
				</div>
			</hbox>
		</template>
	</vbox>
	
	<hlayout>
