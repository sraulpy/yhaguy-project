		<div width="246px"></div>
	</hlayout>
	
	<div height="42px"></div>
	
	<hlayout>
		<div width="95px"></div>
		<div hflex="true">
			<label value="@load(vm.selectedNotaCredito.cliente.pos2.toString().toLowerCase())" maxlength="38" sclass="det"></label>
		</div>	
		<div>
			<label value="@load(vm.selectedNotaCredito.cliente.pos3.toString().toLowerCase())" sclass="det"></label>
		</div>	
		<div width="160px"></div>
	</hlayout>
	
